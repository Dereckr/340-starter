<!-- Content heading -->
<% if (title){ %> <%#EJS Code Block to contain JS if structure if a title exists  %>
    <h1><%= title %></h1>
    <% } else {
        res.redirect('/')
    } %>
    <!-- Message Block -->
     <%- messages() %>

     <% if (errors) { %>
        <ul class="notice">
       <% errors.array().forEach(error => { %>
         <li><%= error.msg %></li>
      <%  }) %>
       </ul>
      <% } %>

    <!-- Login Display -->

    <div>
    <h2> Welcome Happy  <%= accountData.account_firstname %></h2>
    <h3  <% if(accountData.account_type === "Client") { %> hidden <% }%>>Inventory Management</h3>
    <table id="reviewDisplay"></table>
    </div>
    

